{
			"name": "helix",
			"version": "1.0.0",
			"description": "",
			"main": "server/server.js",
			"engines": {
						"node": "8.6.0",
						"npm": "5.4.2"
			},
			"scripts": {
						"dev:server": "nodemon ./server/index.js",
						"watch:client": "cd ./client && webpack --config ./webpack.client.js --watch",
						"build:client": "cd ./client && webpack --config ./webpack.client.js --progress",
						"watch:styles": "cd ./client/styles && compass watch",
						"build:styles": "cd ./client/styles && compass compile -e production --output-style compressed --force",
						"develop": "concurrently --kill-others \"npm run dev:server\" \"npm run watch:client\"",
						"build": "npm run build:styles && npm run build:client",
						"serve": "forever start ./server/index.js > log.txt"
			},
			"author": "",
			"license": "ISC",
